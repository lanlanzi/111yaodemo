<script>
$(function()
{
	ctime();
});
var wait = 5;
var clear = null;
function ctime()
{
      clear = window.setInterval("time()", 1000);
}
function time() 
{
    if(wait == 0) 
    {
        window.clearInterval(clear);
        window.location.href = "/";
    }
    if(wait > 0)
    {
        wait--;
        $(".countTime").html(wait);
    }
    if(wait < 0)
    {
    	wait = 5;
    }
}    
</script>
